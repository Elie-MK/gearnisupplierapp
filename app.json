{
  "expo": {
    "name": "gearnisupplierapp",
    "slug": "gearnisupplierapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/gear.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFC244"
    },
    "fonts": [
      { "asset": "./assets/font/GothamBold.ttf", 
      "fontWeight": "bold" 
    },
      { "asset": "./assets/font/GothamMedium.ttf", 
      "fontWeight": "normal" 
    }
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
